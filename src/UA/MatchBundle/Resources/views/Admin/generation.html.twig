{% extends "UAMatchBundle::layout.html.twig" %}

{% block title %}{{ parent() }} - Ajouter un article{% endblock %}

{% block body %}
    <article class="col-lg-8 col-lg-offset-1">
		<ol class="breadcrumb">
			<li><a href="UABlog_default">{{ 'breadcrumb.home'|trans }}</a></li>
			<li><a href="UAUser_admin">{{ 'breadcrumb.admin'|trans }}</a></li>
			<li class="active">{{ 'breadcrumb.generate'|trans }}</li>
		</ol>

		{{ message|trans({'%game%' : game.name})|raw}}
		<a href="{{ path('UAUser_admin') }}" class="btn btn-default">
			<span class="glyphicon glyphicon-chevron-left"></span>
			{{ 'admin.generate.AdminReturn'|trans }}
		</a>
	</article>
	{{ parent() }}
{% endblock %}